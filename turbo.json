{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["**/.env"],
  "globalEnv": [
    "ANTHROPIC_API_KEY",
    "APP_VARIANT",
    "CLERK_FRONTEND_API_URL",
    "CLERK_SECRET_KEY_DEV",
    "CLERK_SECRET_KEY_PROD",
    "CLERK_SECRET_KEY",
    "CLERK_WEBHOOK_SECRET",
    "CONVEX_DEPLOY_KEY",
    "CONVEX_DEPLOYMENT",
    "CONVEX_URL",
    "CRON_SECRET",
    "DATABASE_URL",
    "ENABLE_EXPERIMENTAL_COREPACK",
    "EXPO_PUBLIC_API_BASE_URL",
    "EXPO_PUBLIC_APP_ENV",
    "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "EXPO_PUBLIC_ONE_SIGNAL_APP_ID_DEV",
    "EXPO_PUBLIC_ONE_SIGNAL_APP_ID_PROD",
    "EXPO_PUBLIC_POSTHOG_API_KEY",
    "GROQ_API_KEY",
    "LANGFUSE_HOST",
    "LANGFUSE_PUBLIC_KEY",
    "LANGFUSE_SECRET_KEY",
    "MAESTRO_TEST_EMAIL_UNLIMITED",
    "MAESTRO_TEST_EMAIL",
    "MAESTRO_TEST_PASSWORD_UNLIMITED",
    "MAESTRO_TEST_PASSWORD",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "ONE_SIGNAL_REST_API_KEY_DEV",
    "ONE_SIGNAL_REST_API_KEY_PROD",
    "OPENAI_API_KEY",
    "OPENROUTER_API_KEY",
    "OPENROUTER_BASE_URL",
    "SENTRY_AUTH_TOKEN",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "STRIPE_PRICE_ID_PERSONAL_ANNUAL",
    "STRIPE_PRICE_ID_PERSONAL_MONTHLY",
    "STRIPE_PRICE_ID_PRO_MONTHLY",
    "STRIPE_SECRET_KEY",
    "STRIPE_WEBHOOK_SECRET"
  ],
  "globalPassThroughEnv": ["COREPACK_HOME"],
  "ui": "stream",
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        ".expo/**",
        ".vercel/output/**",
        "dist/**",
        "*.tsbuildinfo",
        "convex/_generated/**"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "format": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["^topo", "^build"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  }
}
