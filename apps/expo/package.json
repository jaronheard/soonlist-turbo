{
  "name": "@soonlist/expo",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "dev": "expo start",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=19000'",
    "debug.eas": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "eas:build.dev.sim.ios": "eas build --profile development --platform ios",
    "eas:build.dev.device.ios": "eas build --profile development-device --platform ios",
    "eas:build.dev.sim.android": "eas build --profile development --platform android",
    "eas:build.dev.device.android": "eas build --profile development-device --platform android",
    "eas.build.dev.sim": "eas build --profile development --platform ios --platform android",
    "eas.build.dev.device": "eas build --profile development-device --platform ios --platform android",
    "eas:build.prod": "eas build --profile production --platform ios --platform android",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bytescale/sdk": "^3.34.0",
    "@expo/metro-config": "^0.17.6",
    "@shopify/flash-list": "1.6.3",
    "@tanstack/react-query": "^5.29.0",
    "@trpc/client": "11.0.0-rc.334",
    "@trpc/react-query": "11.0.0-rc.334",
    "@trpc/server": "11.0.0-rc.334",
    "expo": "~50.0.14",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.11",
    "expo-file-system": "~16.0.9",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.9",
    "expo-share-intent": "~1.5.1",
    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.12",
    "expo-web-browser": "~12.8.2",
    "nativewind": "~4.0.36",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "~0.73.6",
    "react-native-css-interop": "~0.0.34",
    "react-native-gesture-handler": "~2.14.1",
    "react-native-mlkit-ocr": "^0.3.0",
    "react-native-reanimated": "~3.6.3",
    "react-native-safe-area-context": "~4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-webview": "^13.6.4",
    "superjson": "2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/runtime": "^7.24.4",
    "@soonlist/eslint-config": "workspace:*",
    "@soonlist/prettier-config": "workspace:*",
    "@soonlist/tailwind-config": "workspace:*",
    "@soonlist/tsconfig": "workspace:*",
    "@types/babel__core": "^7.20.5",
    "@types/react": "18.3.0",
    "eslint": "^9.1.1",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "prettier": "@soonlist/prettier-config"
}