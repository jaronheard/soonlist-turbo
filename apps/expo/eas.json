{
  "cli": {
    "version": ">= 4.1.2",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "20.12.0",
      "pnpm": "10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
      "ios": {
        "resourceClass": "medium"
      },
      "channel": "base"
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "buildArtifactPaths": ["ios/build/*"]
      },
      "env": {
        "APP_VARIANT": "development"
      },
      "channel": "development"
    },
    "development-device": {
      "extends": "base",
      "distribution": "internal",
      "developmentClient": true,
      "env": {
        "APP_VARIANT": "development"
      },
      "ios": {
        "buildArtifactPaths": ["ios/build/*"]
      },
      "channel": "development-device"
    },
    "production": {
      "extends": "base",
      "env": {
        "APP_VARIANT": "production"
      },
      "ios": {
        "buildArtifactPaths": ["ios/build/*"]
      },
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6670222216"
      }
    }
  }
}
