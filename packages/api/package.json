{
  "name": "@soonlist/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": "./src/*.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "catalog:",
    "@ai-sdk/openai": "catalog:",
    "@clerk/nextjs": "catalog:",
    "@sentry/nextjs": "catalog:",
    "@sentry/node": "catalog:",
    "@soonlist/cal": "workspace:*",
    "@soonlist/db": "workspace:*",
    "@soonlist/validators": "workspace:*",
    "@trpc/server": "catalog:",
    "@vercel/functions": "catalog:",
    "ai": "catalog:",
    "date-fns-tz": "catalog:",
    "expo-server-sdk": "catalog:",
    "langfuse": "catalog:",
    "nanoid": "catalog:",
    "posthog-node": "catalog:",
    "stripe": "catalog:",
    "superjson": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@soonlist/eslint-config": "workspace:*",
    "@soonlist/prettier-config": "workspace:*",
    "@soonlist/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@soonlist/prettier-config"
}
