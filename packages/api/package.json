{
  "name": "@soonlist/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": "./src/*.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.18",
    "@clerk/nextjs": "^5.0.8",
    "@soonlist/cal": "workspace:*",
    "@soonlist/db": "workspace:*",
    "@soonlist/validators": "workspace:*",
    "@trpc/server": "11.0.0-rc.334",
    "@vercel/functions": "^1.0.2",
    "ai": "^3.1.22",
    "expo-server-sdk": "^3.10.0",
    "langfuse": "^3.11.0",
    "nanoid": "^5.0.7",
    "stripe": "^15.9.0",
    "superjson": "2.2.1",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@soonlist/eslint-config": "workspace:*",
    "@soonlist/prettier-config": "workspace:*",
    "@soonlist/tsconfig": "workspace:*",
    "eslint": "^9.1.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "prettier": "@soonlist/prettier-config"
}
